                <div class="carousel-item">
                    <img id="carouselImg3" src="/images/carouselMobile/carousel3_5.jpg" class="img-fluid" alt="">
                </div>

            </div>
        </div>



        <!-- Section 1 -->



        <div class="container-fluid secondWrapper">


            <section id="cont2" class="container-fluid text-center" style="overflow: hidden;">
                <p>FEATURING</p>
                <h4>UNIXO HAND-PAINTED SNEAKERS</h4>
                <br> <br>
                <br>
                <div class="row justify-content-center">

                    <% for (let i=0; i < 4; i++) { %>
                        <div class="col-sm-6 col-lg-3 homeKicks" data-aos="fade-in" data-aos-delay="<%=150*i%>"
                            data-aos-duration="300">
                            <a href="/shop/collections/11001/<%=kicks[i].shopId%>">
                                <div class="img-zoom-cont">
                                    <img src="<%= kicks[i].image[0] %>" alt="" class="img-fluid homepageKicks">
                                </div>
                                <p>Air Force 1 <%=kicks[i].name%>
                                </p>
                            </a>
                            <p class="cont2Price">FROM $<%=kicks[i].price%> USD</p>
                        </div>
                        <% } %>

                </div>





                <div class="buttonViewAll" id="button-2" onclick="javascript: location.href='/shop/collections'">
                    <div id="slide"></div>
                    <a href="/shop/collections">View all products</a>
                </div>

        </div>
        </section>



    </div>

    <!-- Section 3 -->

    <section id="cont3" class="container-fluid" data-aos="fade-in">

        <div id="cont3Row1" class="row text-center align-items-center align-content-center justify-content-center">

