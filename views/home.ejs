                            <a href="/shop/collections/11001">SHOP NOW</a>
                        </div>
                    </div>

                </div>


                <div class="carousel-item">
                    <div class="container-fluid containerImg">
                        <div class="row text-center">
                            <div class="col-12">
                                <img src="/images/section3/tp1.png" alt="" class="img-fluid themesImg">
                            </div>
                        </div>
                    </div>

                    <div class="container-fluid containerText text-center">
                        <h4 class="mb-2">Young Blood</h4>
                        <div class="buttonCustom3 button-4"
                            onclick="javascript: location.href='/shop/collections/11002'">
                            <div class="circle"></div>
                            <a href="/shop/collections/11002">SHOP NOW</a>
                        </div>
                    </div>

                </div>


                <div class="carousel-item">
                    <div class="container-fluid containerImg">
                        <div class="row text-center">
                            <div class="col-12">
                                <img src="/images/section3/tp1.png" alt="" class="img-fluid themesImg">
                            </div>
                        </div>
                    </div>

                    <div class="container-fluid containerText text-center">
                        <h4 class="mb-2">Dreams of Sky</h4>
                        <div class="buttonCustom3 button-4"
                            onclick="javascript: location.href='/shop/collections/11003'">
                            <div class="circleCS4"></div>
                            <a href="/shop/collections/11003">SHOP NOW</a>
                        </div>
                    </div>

                </div>

            </div>

            <a href="#carousel2" class="carousel-control-prev" role="button" data-slide="prev">
                <span class="sr-only">Previous</span>
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            </a>

            <a href="#carousel2" class="carousel-control-next" role="button" data-slide="next">
                <span class="sr-only">Next</span>
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
            </a>

        </div>

    </div>






