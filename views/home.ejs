                <li data-target="#slides" data-slide-to="1"></li>
                <li data-target="#slides" data-slide-to="2"></li>
            </ul>

            <div class="carousel-inner">

                <div class="carousel-item active">
                    <img id="carouselImg1" src="/images/carouselMobile/carousel1_5.jpg" class="img-fluid" alt=""
                        style=" filter: brightness(80%);">
                    <div class="carousel-caption">
                        <h1 class="display-2">UNIXO</h1>
                        <h3>Art that you can wear.</h3>
                        <div class="buttonCustom5 button-6">
                            <div class="circle3"></div>
                            <a href="#">SHOP NOW</a>
                        </div>
                    </div>
                </div>

                <div class="carousel-item">
                    <img id="carouselImg2" src="/images/carouselMobile/carousel2_5.jpg" class="img-fluid" alt="">
